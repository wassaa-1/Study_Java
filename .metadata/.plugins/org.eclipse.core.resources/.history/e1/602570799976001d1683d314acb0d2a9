package step05oop.step01class;

public class Mouse {
	int age;
	String name;
	String country;
	String religion;
	String address;
	
	void sing() {
		System.out.println("찍찍?!");
	}
	
	// 기본 생성자 메서드. 작성하지 않아도 스스로 생성됨
//	public Mouse(){
//		System.out.println("Mouse() 호출");
//	}
	
	public Mouse(int age, String name, String country) {
		this.age = age;
		this.name = name;
		this.country = country;
	}
}
